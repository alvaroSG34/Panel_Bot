# Copiar credenciales de la base de datos del bot
# Cambiar la ruta si el proyecto del bot está en otra ubicación

# PostgreSQL Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=enrollment_db
DB_USER=postgres
DB_PASSWORD=your_password_here
DB_SSL=false

# JWT Secrets
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=15m
REFRESH_TOKEN_EXPIRES_IN=7d

# Server
PORT=3000
NODE_ENV=development

# CORS (para desarrollo con frontend en puerto diferente)
CORS_ORIGIN=http://localhost:5173
